name: Demo WorkFlow
 
on:
  workflow_dispatch:
  workflow_call:
  
env:
  CX_VENV_PYTHON: python
  CX_VENV_PIP: pip
jobs:
  initialize:
    runs-on:ubuntu-latest
    steps:
      - name: Checkout Source
        uses: actions/checkout@v2        
      - name: Execute command
        id: init_config
        run: ${{ env.CX_VENV_PYTHON }} hello.py
